{
  "manifest_version": 2,
  "name": "Jitsi Emojis",
  "version": "0.1.2",
  "author": "Guillem Palomar",
  "description": "Complete emojis for Jitsi chat application.",
  "icons": {
    "16": "icons/icon-extension-16.png",
    "48": "icons/icon-extension-48.png",
    "128": "icons/icon-extension-128.png"
  },
  "web_accessible_resources": [
    "icons/**"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://meet.jit.si/*"
      ],
      "js": [
        "emojis.js",
        "contentScript.js"
      ]
    }
  ],
  "permissions": [ "https://meet.jit.si/*", "activeTab" ],
  "content_security_policy": "default-src 'self'; script-src 'self' 'unsafe-eval'; connect-src data:;"
}